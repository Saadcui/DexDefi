# Hardhat Sepolia deployment (optional)
# Copy this file to .env (root) and fill values.

# Example: https://sepolia.infura.io/v3/<key>  or Alchemy / your RPC
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY

# Private key of the deployer account (NO 0x prefix also works in Hardhat)
PRIVATE_KEY=0xYOUR_PRIVATE_KEY

# Optional
ETHERSCAN_API_KEY=YOUR_ETHERSCAN_KEY

